{
  "name": "jumpy",
  "main": "./dist/jumpy",
  "version": "5.2.0",
  "description": "An Atom package that creates dynamic hotkeys to jump around files across visible panes.",
  "keywords": [
    "hotkey",
    "hotkeys",
    "key bindings",
    "cursor",
    "jump",
    "panes",
    "shortcuts",
    "navigation",
    "productivity",
    "mouseless",
    "plugin",
    "extension"
  ],
  "scripts": {
    "clean": "rm -r dist/*",
    "link-js": "mkdir -p dist/labelers && ln -sf ../../lib/labelers/words-regex-match-all-the-things.js dist/labelers/",
    "build": "npm run link-js && node_modules/.bin/tsc -p lib",
    "clean-build": "npm run clean && npm run build",
    "test": "apm test"
  },
  "repository": "https://github.com/DavidLGoldberg/jumpy",
  "license": "MIT",
  "engines": {
    "atom": ">0.50.0"
  },
  "activationCommands": {"atom-workspace": "jumpy:toggle"},
  "atomTestRunner": "./spec/jasmine3-runner",
  "consumedServices": {
    "status-bar": {
      "versions": {
        "^1.0.0": "consumeStatusBar"
      }
    },
    "vim-mode-plus": {
      "versions": {
        "0.1.0": "consumeVimModePlus"
      }
    }
  },
  "dependencies": {
    "javascript-state-machine": "2.4.0",
    "lodash": "4.17.10",
    "xstate": "^3.3.2"
  },
  "devDependencies": {
    "@types/atom": "1.27.0",
    "@types/javascript-state-machine": "2.4.1",
    "@types/lodash": "4.14.109",
    "atom-jasmine3-test-runner": "^3.1.0",
    "space-pen": "^5.1.2",
    "tslib": "^1.9.2",
    "typescript": "2.9.2"
  }
}
